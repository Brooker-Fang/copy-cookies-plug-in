{
  "manifest_version": 3,
  "name": "Copy Cookies Extensions",
  "description": "Copy Cookies Extension",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "cookies.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "scripting",
    "cookies",
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}